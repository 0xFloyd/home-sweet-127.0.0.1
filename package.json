    "express": "^4.17.1",
    "fast-speedtest-api": "^0.3.2",
    "node-fetch": "^2.6.0",
    "request-ip": "^2.1.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^2.0.2"
  }
}
